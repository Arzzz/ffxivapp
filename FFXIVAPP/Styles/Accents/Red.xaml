<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="HighlightDarkColor">#FFcb0000</Color>
    <Color x:Key="HighlightLightColor">#FFcb0000</Color>
    <Color x:Key="AccentColor">#CCcb0000</Color>
    <Color x:Key="AccentColor2">#99cb0000</Color>
    <Color x:Key="AccentColor3">#66cb0000</Color>
    <Color x:Key="AccentColor4">#33cb0000</Color>
    <LinearGradientBrush x:Key="BrandingBrush" StartPoint="1.002,0.5" EndPoint="0.001,0.5">
        <GradientStop Offset="0" Color="{DynamicResource HighlightDarkColor}" />
        <GradientStop Offset="1" Color="#FFD4D115" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{DynamicResource AccentColor}" />
</ResourceDictionary>