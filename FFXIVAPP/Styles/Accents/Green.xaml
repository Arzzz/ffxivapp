<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="HighlightDarkColor">#FF48cb00</Color>
    <Color x:Key="HighlightLightColor">#FF48cb00</Color>
    <Color x:Key="AccentColor">#CC48cb00</Color>
    <Color x:Key="AccentColor2">#9948cb00</Color>
    <Color x:Key="AccentColor3">#6648cb00</Color>
    <Color x:Key="AccentColor4">#3348cb00</Color>
    <LinearGradientBrush x:Key="BrandingBrush" StartPoint="1.002,0.5" EndPoint="0.001,0.5">
        <GradientStop Offset="0" Color="{DynamicResource HighlightDarkColor}" />
        <GradientStop Offset="1" Color="#FF98370F" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{DynamicResource AccentColor}" />
</ResourceDictionary>