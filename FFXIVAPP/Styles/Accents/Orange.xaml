<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="HighlightDarkColor">#FF9e5608</Color>
    <Color x:Key="HighlightLightColor">#FF9e5608</Color>
    <Color x:Key="AccentDarkColor">#FFde872a</Color>
    <Color x:Key="AccentColor">#CCff9829</Color>
    <Color x:Key="AccentColor2">#99ff9829</Color>
    <Color x:Key="AccentColor3">#66ff9829</Color>
    <Color x:Key="AccentColor4">#33ff9829</Color>
    <LinearGradientBrush x:Key="BrandingBrush" StartPoint="1.002,0.5" EndPoint="0.001,0.5">
        <GradientStop Offset="0" Color="{DynamicResource HighlightDarkColor}" />
        <GradientStop Offset="1" Color="#FF5FC316" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{DynamicResource AccentColor}" />
</ResourceDictionary>