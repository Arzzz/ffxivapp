<UserControl x:Class="FFXIVAPP.Client.Controls.Parse.PlayerInfoBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:CommonConverters="clr-namespace:FFXIVAPP.Common.Converters;assembly=FFXIVAPP.Common"
             xmlns:Converters="clr-namespace:FFXIVAPP.Client.Converters"
             xmlns:Custom="http://metro.mahapps.com/winfx/xaml/shared"
             xmlns:Parse="clr-namespace:FFXIVAPP.Client.ViewModels.Parse"
             xmlns:PluginProperties="clr-namespace:FFXIVAPP.Client.SettingsProviders.Parse"
             xmlns:Properties="clr-namespace:FFXIVAPP.Client.Properties"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d">

    <UserControl.Resources>
        <CommonConverters:VisibilityConverter x:Key="VisibilityConverter" />
        <x:Static x:Key="PluginSettings"
                  Member="PluginProperties:Settings.Default" />
        <Custom:ToUpperConverter x:Key="ToUpperConverter" />
        <x:Static x:Key="PluginViewModel"
                  Member="Parse:PluginViewModel.Instance" />
        <Converters:NameToAvatarConverter x:Key="NameToAvatarConverter" />
        <Style x:Key="StatLabelStyle">
            <Setter Property="Control.Padding" Value="0" />
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        </Style>
        <Style x:Key="CommaNumberStyle"
               BasedOn="{StaticResource StatLabelStyle}">
            <Setter Property="ContentControl.ContentStringFormat" Value="{}{0:N0}" />
        </Style>
        <Style x:Key="PercentStyle"
               BasedOn="{StaticResource StatLabelStyle}">
            <Setter Property="ContentControl.ContentStringFormat" Value="{}{0:P}" />
        </Style>
        <Style x:Key="WhiteStatTextBlockStyle">
            <Setter Property="Control.Padding" Value="0" />
            <Setter Property="Control.Foreground" Value="White" />
            <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        </Style>
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot"
          Width="270"
          Margin="5">
        <Grid.Triggers>
            <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                <BeginStoryboard x:Name="LayoutRootBeginStoryBoard">
                    <Storyboard x:Name="LayoutRootStoryBoard">
                        <DoubleAnimation AutoReverse="false"
                                         BeginTime="0:0:0"
                                         Duration="0:0:.5"
                                         From="0"
                                         Storyboard.TargetName="LayoutRoot"
                                         Storyboard.TargetProperty="(UIElement.Opacity)"
                                         To="1" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
        </Grid.Triggers>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="40" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="5" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="5" />
            <RowDefinition Height="5" />
            <RowDefinition Height="20" />
            <RowDefinition Height="25" />
            <RowDefinition Height="*" />
            <RowDefinition Height="5" />
        </Grid.RowDefinitions>
        <Border Grid.Row="0"
                Grid.RowSpan="3"
                Grid.Column="0"
                Grid.ColumnSpan="5"
                Background="{DynamicResource AccentColorBrush}"
                BorderBrush="{DynamicResource AccentColorBrush}"
                BorderThickness="1" />
        <Image Grid.Row="1"
               Grid.RowSpan="3"
               Grid.Column="2"
               Width="40"
               Height="40"
               RenderOptions.BitmapScalingMode="HighQuality">
            <Image.Source>
                <MultiBinding Converter="{StaticResource NameToAvatarConverter}">
                    <Binding RelativeSource="{RelativeSource Self}" />
                    <Binding Path="Name" />
                </MultiBinding>
            </Image.Source>
        </Image>
        <DockPanel Grid.Row="0"
                   Grid.RowSpan="3"
                   Grid.Column="3"
                   LastChildFill="True">
            <DockPanel DockPanel.Dock="Right">
                <TextBlock Margin="5 0"
                           VerticalAlignment="Center"
                           FontSize="20"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="{Binding ActivePercent,
                                          StringFormat=\{0:P0\}}" />
                <TextBlock Margin="4"
                           VerticalAlignment="Bottom"
                           Padding="0"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="{Binding Locale[parse_ActiveText],
                                          Source={StaticResource AppProperties},
                                          Converter={StaticResource ToUpperConverter}}" />
                <TextBlock Margin="5 0"
                           VerticalAlignment="Center"
                           FontSize="20"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="{Binding CombinedDPS,
                                          StringFormat=\{0:N0\}}"
                           ToolTip="(DPS + DOTPS)" />
                <TextBlock Margin="4"
                           VerticalAlignment="Bottom"
                           Padding="0"
                           Style="{StaticResource WhiteStatTextBlockStyle}"
                           Text="DPS" />
            </DockPanel>
            <DockPanel />
        </DockPanel>
        <TextBlock Grid.Row="3"
                   Grid.Column="3"
                   VerticalAlignment="Center"
                   Foreground="{DynamicResource LabelTextBrush}"
                   Padding="5 0"
                   Text="{Binding Name,
                                  Converter={StaticResource ToUpperConverter}}" />
        <Border Grid.Row="3"
                Grid.Column="0"
                Grid.ColumnSpan="5"
                BorderBrush="{DynamicResource AccentColorBrush}"
                BorderThickness="1 0" />
        <Border Grid.Row="4"
                Grid.RowSpan="3"
                Grid.Column="0"
                Grid.ColumnSpan="5"
                BorderBrush="{DynamicResource AccentColorBrush}"
                BorderThickness="1 0 1 1">
